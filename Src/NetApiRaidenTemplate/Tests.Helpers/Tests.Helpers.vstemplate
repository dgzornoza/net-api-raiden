<VSTemplate Version="3.0.0" xmlns="http://schemas.microsoft.com/developer/vstemplate/2005" Type="Project">
    <TemplateData>
        <Name>Tests.Helpers</Name>
        <Description>NetApiRaiden Tests Helpers Project</Description>
        <ProjectType>CSharp</ProjectType>
        <ProjectSubType>
        </ProjectSubType>
        <SortOrder>1000</SortOrder>
        <CreateNewFolder>true</CreateNewFolder>
        <DefaultName>Tests.Helpers</DefaultName>
        <ProvideDefaultName>true</ProvideDefaultName>
        <LocationField>Enabled</LocationField>
        <EnableLocationBrowseButton>true</EnableLocationBrowseButton>
        <CreateInPlace>true</CreateInPlace>
    </TemplateData>
    <TemplateContent>
        <Project TargetFileName="Tests.Helpers.csproj" File="Tests.Helpers.csproj" ReplaceParameters="true">
            <Folder Name="Extensions" TargetFolderName="Extensions">
                <ProjectItem ReplaceParameters="true" TargetFileName="EntityExtension.cs">EntityExtension.cs</ProjectItem>
            </Folder>
            <Folder Name="Mocks" TargetFolderName="Mocks">
                <ProjectItem ReplaceParameters="true" TargetFileName="MockValueChecker.cs">MockValueChecker.cs</ProjectItem>
            </Folder>
        </Project>
    </TemplateContent>
</VSTemplate>